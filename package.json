    "scripts": {
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "node dist/main",
        "dev": "cross-env NODE_ENV=development yarn start:dev",
        "build:dev": "cross-env NODE_ENV=staging yarn build",
        "build:prod": "cross-env NODE_ENV=production yarn build",
        "build": "nest build",
        "start:dev": "nest start --watch",
        "start:prod": "nest start",
        "start:debug": "nest start --debug --watch",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "cross-env ts-node ./node_modules/typeorm/cli",
        "typeorm:run-mig": "cross-env npm run typeorm migration:run -- -d ./src/database/typeOrm.config.ts",
        "typeorm:generate-mig": "cross-env npm run typeorm -- -d ./src/database/typeOrm.config.ts migration:generate ./src/database/migrations/%npm_config_name%",
        "typeorm:create-mig": "cross-env npm run typeorm -- migration:create ./src/database/migrations/%npm_config_name%",
        "typeorm:revert-mig": "cross-env npm run typeorm -- -d ./src/database/typeOrm.config.ts migration:revert"
    },
